
## Daemon processes to be run by init.
##

service ueventd /sbin/ueventd
    class core
    #critical
    oneshot

# service kisd /system/bin/kisd

service healthd /sbin/healthd
    class core
    critical
    seclabel u:r:healthd:s0
    oneshot

# service healthd-charger /sbin/healthd -n
#     class charger
#     critical
#     seclabel u:r:healthd:s0

# on property:selinux.reload_policy=1
#     write /proc/bootprof "on property:selinux.reload_policy=1"
#     restart ueventd
#     restart installd

service console /system/bin/sh
    class core
    disabled
    console
    user root
    group log
    oneshot

service telnetd30 /system/xbin/busybox telnetd -l /system/bin/sh
    class main
    disabled
    oneshot

# # adbd is controlled via property triggers in init.<platform>.usb.rc
# service adbd /sbin/adbd
#     class core
#     socket adbd stream 660 system system
#     disabled

service drvbd /system/bin/drvbd
    class main
    user system
    group system
    oneshot

# service debuggerd /system/bin/debuggerd
#     class main

# service mobile_log_d /system/bin/mobile_log_d
#     class main

service servicemanager /system/bin/servicemanager
    class core
    user system
    group system
    oneshot
    #critical
    #onrestart restart healthd
    #onrestart restart zygote
    #onrestart restart media
    #onrestart restart surfaceflinger
    #onrestart restart drm
    #onrestart restart aal

# Required to run apk
service vold /system/bin/vold
    class core
    socket vold stream 0660 root mount
    ioprio be 2
    oneshot

service media /system/bin/mediaserver
    class main
    user root
    group audio camera graphics inet net_bt net_bt_admin net_bw_acct drmrpc media sdcard_r nvram ccci radio system
    ioprio rt 4
    oneshot

service netd /system/bin/netd
    class main
    socket netd stream 0660 root system
    socket dnsproxyd stream 0660 root inet
    socket mdns stream 0660 root system
    oneshot

# service netdiag /system/bin/netdiag
#     class main
#     socket netdiag stream 0664 root inet

service nvram_daemon /system/bin/nvram_daemon
    class main
    user nvram
    group nvram system
    oneshot

